package com.sof.billing.disputesproject;


rule "ApprovalRequired" ruleflow-group "SOFBillingRFG"

when
	$disputeInfo : DisputeInfoObject( disputeAmount > 400)

then
	$disputeInfo.setApprovalRequired(true);
	System.out.println("Triggered approval rule with object=" + $disputeInfo.getApprovalRequired());

end
